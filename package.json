{
  "name": "hmrc-frontend",
  "version": "6.108.0",
  "description": "Design patterns for HMRC frontends",
  "scripts": {
    "start": "gulp dev",
    "test:compatibility": " jest tasks/gulp/__tests__/check-compatibility-with-govuk-frontend-version.test.js",
    "test:build:package": "jest tasks/gulp/__tests__/after-build-package.test.js",
    "test:build:dist": "jest tasks/gulp/__tests__/after-build-dist.test.js",
    "test:build:webjar": "jest tasks/gulp/__tests__/after-build-webjar.jsdom.test.js",
    "build:package": "gulp buildPackage --destination 'package' && npm run test:build:package",
    "build:dist": "gulp buildDist --destination 'dist' && npm run test:build:dist",
    "build:webjar": "gulp buildWebjar && npm run test:build:webjar",
    "publish-local:webjar": "gulp publishLocalWebjar",
    "gulp-tasks": "gulp --tasks",
    "build": "npm run build:dist && npm run build:package",
    "lint": "stylelint 'src/**/*.scss' && eslint .",
    "lint:fix": "stylelint 'src/**/*.scss' --fix && eslint --fix .",
    "test": "npm run build:dist && npm run test:compatibility && jest src && npm run lint",
    "test:update-snapshots": "jest src -u",
    "test:backstop": "gulp backstopTest",
    "test:backstop-approve": "gulp backstop:approve",
    "audit": "better-npm-audit audit",
    "prepare": "husky install",
    "postinstall": "node check-compatibility-with-govuk-frontend-version.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hmrc/hmrc-frontend.git"
  },
  "keywords": [
    "HMRC",
    "frontend",
    "design patterns"
  ],
  "author": {
    "name": "HM Revenue and Customs",
    "email": "platform-ui@digital.hmrc.gov.uk"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/hmrc/hmrc-frontend/issues"
  },
  "homepage": "https://github.com/hmrc/hmrc-frontend#readme",
  "dependencies": {
    "accessible-autocomplete": "^3.0.1",
    "govuk-frontend": "^5.13.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.21.5",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "adm-zip": "^0.5.5",
    "ajv": "^8.12.0",
    "autoprefixer": "^10.4.21",
    "backstopjs": "6.3.25",
    "better-npm-audit": "^3.7.3",
    "cheerio": "^1.0.0-rc.10",
    "cssnano": "^7.1.1",
    "del": "^5.1.0",
    "eslint": "^8.8.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "expect-puppeteer": "^10.1.0",
    "express": "^4.21.0",
    "glob": "^10.5.0",
    "gulp": "5.0.0",
    "gulp-better-rollup": "^4.0.1",
    "gulp-eol": "^0.2.0",
    "gulp-filter": "^7.0.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^9.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.4",
    "gulp-sass": "^6.0.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglify-es": "^3.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-axe": "^3.5.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-puppeteer": "^11.0.0",
    "jest-serializer-html": "^5.0.0",
    "js-yaml": "^4.1.1",
    "mkdirp": "^3.0.1",
    "node-fetch": "^3.3.1",
    "node-sass": "^9.0.0",
    "nodemon": "^3.1.10",
    "nunjucks": "^3.2.4",
    "playwright-chromium": "^1.49.1",
    "playwright-core": "^1.49.1",
    "postcss": "^8.0.0",
    "postcss-scss": "^4.0.9",
    "puppeteer": "^23.4.1",
    "recursive-readdir": "^2.2.2",
    "remarkable": "^1.7.4",
    "rollup": "^4.50.1",
    "sass": "1.77.6",
    "sinon": "^18.0.1",
    "standard": "^17.0.0",
    "stylelint": "^16.21.0",
    "stylelint-config-gds": "^2.0.0",
    "yargs": "^15.3.1"
  },
  "browserslist": [
    "last 2 versions",
    "ie 8",
    "ie 9",
    "iOS 9"
  ],
  "overrides": {
    "playwright": "$playwright-core",
    "puppeteer": "$puppeteer",
    "rollup": "$rollup",
    "form-data": "^4.0.4",
    "tough-cookie": "^4.1.3",
    "autolinker": "^4.1.5",
    "gulp-header": "^2.0.9"
  }
}
